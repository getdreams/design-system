    "target": "ESNext",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "reactNamespace": "React",
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["*"],
      "@divriots/tailwindcss/*": ["*"]
    }
  }
}
